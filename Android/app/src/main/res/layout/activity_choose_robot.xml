<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChooseRobot">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="207dp"
        android:layout_height="53dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="270dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="254dp"
        android:fontFamily="@font/fredoka_one"
        android:text="@string/texto_de_bienvenida"
        android:textDirection="firstStrongRtl"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="346dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="225dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="160dp"
        android:fontFamily="@font/fredoka_one"
        android:text="@string/texto_seleccion_de_robot"
        android:textDirection="firstStrongRtl"
        android:textSize="24sp" />

    <ImageButton
        android:id="@+id/imgbtn_1"
        android:layout_width="200dp"
        android:layout_height="159dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="106dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="101dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/nao_int1_01" />

    <ImageButton
        android:id="@+id/imgbtn_2"
        android:layout_width="200dp"
        android:layout_height="159dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="135dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="118dp"
        android:layout_marginBottom="103dp"
        android:layout_toEndOf="@+id/imgbtn_1"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/robot_con_los_ojos_y_las_ruedas_en_un_fondo_blanco_internacional_artificial_113929772" />

</RelativeLayout>